{
    "name": "drupal/provision",
    "description": "Drush commands powering the back-end of Aegir",
    "type": "drupal-drush",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name":      "Aegir maintainers",
            "homepage":  "http://docs.aegirproject.org/en/3.x/community/core-team/"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/provision",
        "docs": "http://docs.aegirproject.org/"
    },
    "require": {
        "devshop/git-traits": "dev-feature/composer/git-traits",
        "eloquent/composer-config-reader": "^3.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "symfony/process": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "provision.inc"
        ],
        "psr-0": {
            "Provision_": [
                "Provision"
            ]
        }
    },
    "require-dev": {
        "overtrue/phplint": "^1.2"
    }
}
